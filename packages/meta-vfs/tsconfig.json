{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@modou/meta-vfs": ["src"],
      "@modou/meta-vfs/*": ["src/*", "*"]
    },
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
