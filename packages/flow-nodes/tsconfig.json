{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@modou/flow-nodes": ["src"],
      "@modou/flow-nodes/*": ["src/*", "*"]
    },
    "lib": [
      "decorators",
      "dom",
      "es5",
      "scripthost",
      "es2015.promise"
    ]
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
